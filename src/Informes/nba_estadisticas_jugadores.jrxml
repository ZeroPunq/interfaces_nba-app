	<parameter name="nombre_equipo" class="java.lang.String"/>
	<parameter name="temporada" class="java.lang.String"/>
	<query language="sql"><![CDATA[SELECT 
    jugadores.Nombre AS Jugador,
    equipos.Nombre AS Equipo,
    estadisticas.temporada AS Temporada,
    estadisticas.Puntos_por_partido,
    estadisticas.Rebotes_por_partido,
    estadisticas.Asistencias_por_partido,
    estadisticas.Tapones_por_partido
FROM jugadores
INNER JOIN equipos ON jugadores.Nombre_equipo = equipos.Nombre
INNER JOIN estadisticas ON jugadores.codigo = estadisticas.jugador
WHERE estadisticas.temporada = $P{temporada} 

]]></query>
