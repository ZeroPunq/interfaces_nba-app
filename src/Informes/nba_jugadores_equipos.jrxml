	<parameter name="nombre_equipo" class="java.lang.String"/>
	<query language="sql"><![CDATA[SELECT 
    jugadores.Nombre AS Jugador, 
    jugadores.Posicion, 
    jugadores.Altura, 
    jugadores.Peso, 
    jugadores.Procedencia, 
    equipos.Nombre AS Equipo, 
    equipos.Ciudad, 
    equipos.Conferencia, 
    equipos.Division
FROM jugadores
INNER JOIN equipos 
    ON jugadores.Nombre_equipo = equipos.Nombre
    WHERE equipos.Nombre = $P{nombre_equipo} 
]]></query>
